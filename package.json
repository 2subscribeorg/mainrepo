{
  "name": "2subscribe",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:e2e": "playwright test",
    "test:plaid": "vitest --config vitest.plaid.config.ts --run",
    "test:plaid-quick": "tsx scripts/test-plaid-backend.ts",
    "test:admin": "vitest run src/services/AdminService.test.ts tests/integration/superadmin.test.ts --reporter=verbose",
    "test:admin-unit": "vitest run src/services/AdminService.test.ts",
    "test:admin-integration": "vitest run tests/integration/superadmin.test.ts",
    "test:admin-components": "vitest run src/components/admin/*.test.ts",
    "test:admin-all": "vitest run src/services/AdminService.test.ts tests/integration/superadmin.test.ts src/components/admin/*.test.ts --reporter=verbose",
    "lint": "eslint . --ext .vue,.ts,.tsx --max-warnings 10",
    "type-check": "vue-tsc --noEmit",
    "format": "prettier --write .",
    "cap:sync": "cap sync",
    "cap:open:android": "cap open android",
    "cap:open:ios": "cap open ios",
    "cap:run:android": "cap run android",
    "cap:run:ios": "cap run ios"
  },
  "dependencies": {
    "@capacitor/android": "^6.1.2",
    "@capacitor/core": "^6.1.2",
    "@capacitor/ios": "^6.1.2",
    "@capacitor/local-notifications": "^6.1.0",
    "firebase": "^12.5.0",
    "idb": "^8.0.0",
    "pinia": "^2.2.2",
    "plaid": "^39.1.0",
    "react-plaid-link": "^4.1.1",
    "vue": "^3.4.38",
    "vue-router": "^4.4.3",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@capacitor/cli": "^6.1.2",
    "@faker-js/faker": "^9.0.0",
    "@playwright/test": "^1.47.0",
    "@types/node": "^22.5.1",
    "@typescript-eslint/eslint-plugin": "^8.4.0",
    "@typescript-eslint/parser": "^8.4.0",
    "@vitejs/plugin-vue": "^5.1.3",
    "@vue/test-utils": "^2.4.6",
    "autoprefixer": "^10.4.20",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-vue": "^9.27.0",
    "fake-indexeddb": "^6.2.4",
    "jsdom": "^25.0.0",
    "postcss": "^8.4.45",
    "prettier": "^3.3.3",
    "tailwindcss": "^3.4.10",
    "typescript": "^5.5.4",
    "vite": "^5.4.3",
    "vitest": "^2.0.5",
    "vue-tsc": "^2.1.6"
  }
}
